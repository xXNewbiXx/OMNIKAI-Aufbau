{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Omni Analytics Events",
  "type": "object",
  "properties": {
    "event": {
      "type": "string"
    },
    "ts": {
      "type": "string",
      "format": "date-time"
    },
    "source": {
      "type": "string"
    },
    "user_id": {
      "type": "string"
    },
    "session_id": {
      "type": "string"
    },
    "utm": {
      "type": "object",
      "properties": {
        "source": {
          "type": "string"
        },
        "medium": {
          "type": "string"
        },
        "campaign": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "term": {
          "type": "string"
        }
      }
    },
    "metrics": {
      "type": "object"
    },
    "metadata": {
      "type": "object"
    }
  },
  "required": [
    "event",
    "ts",
    "source"
  ]
}